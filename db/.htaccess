<IfModule mod_rewrite.c>
    RewriteEngine On
    RewriteBase /talcual/db/
    RewriteRule ^index\.html$ - [L]
    RewriteCond %{REQUEST_FILENAME} !-f
    RewriteCond %{REQUEST_FILENAME} !-d
    RewriteRule . index.html [L]
</IfModule>

<FilesMatch "\.db$">
    Deny from all
</FilesMatch>

<FilesMatch "\.sql$">
    Deny from all
</FilesMatch>

<FilesMatch "^(admin-api|admin)">
    Allow from all
</FilesMatch>